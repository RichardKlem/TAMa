<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_dashboard">

    <fragment
        android:id="@+id/navigation_events"
        android:name="com.example.tama.ui.events.EventsFragment"
        android:label="@string/title_events"
        tools:layout="@layout/fragment_events" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.tama.ui.locations.LocationFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_location">
        <action
            android:id="@+id/action_navigation_dashboard_to_navigation_events"
            app:destination="@id/navigation_events" />
    </fragment>

</navigation>
